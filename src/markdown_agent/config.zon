    .default_settings = .{
        .text_wrap_width = 80,
        .heading_style = "atx",
        .list_style = "dash",
        .code_fence_style = "backtick",
        .table_alignment = "auto",
        .front_matter_format = "yaml",
        .toc_style = "github",
        .link_style = "reference",
        .rfc_2119_compliance = true,
        .requirement_keywords_style = "all_caps",
        .normative_reference_style = "bracketed",
    },
    .context_aware_configuration = .{
        .adaptive_behavior = .{
            .auto_detect_context = true,
            .learn_from_usage = true,
            .override_hierarchy = .{ "user", "project", "document_type", "global" },
        },
        .by_user_expertise = .{
            .beginner = .{
                .validation_level = "helpful",
                .guidance_level = "high",
                .template_suggestions = true,
                .explain_reasoning = "always",
                .text_wrap_width = 100,
                .require_confirmation = "on_major_changes",
            },
            .intermediate = .{
                .validation_level = "standard",
                .guidance_level = "medium",
                .template_suggestions = true,
                .explain_reasoning = "when_requested",
                .text_wrap_width = 80,
                .require_confirmation = "on_destructive_changes",
            },
            .expert = .{
                .validation_level = "minimal",
                .guidance_level = "low",
                .template_suggestions = false,
                .explain_reasoning = "when_requested",
                .text_wrap_width = 80,
                .require_confirmation = "never",
            },
        },
        .by_project_type = .{
            .technical_documentation = .{
                .preferred_templates = .{ "api_documentation", "tutorial", "specification" },
                .validation_focus = .{ "code_syntax", "link_integrity", "technical_accuracy" },
                .required_sections = .{ "overview", "examples", "parameters" },
                .style_preferences = .{
                    .code_fence_style = "backtick",
                    .heading_style = "atx",
                    .include_toc = true,
                },
            },
            .academic_writing = .{
                .preferred_templates = .{ "research_paper", "literature_review", "thesis_chapter" },
                .validation_focus = .{ "citation_format", "structure_hierarchy", "academic_style" },
                .required_sections = .{ "abstract", "introduction", "methodology", "conclusion", "references" },
                .style_preferences = .{
                    .citation_style = "apa",
                    .heading_numbering = true,
                    .max_line_length = 90,
                },
            },
            .content_publishing = .{
                .preferred_templates = .{ "blog_post", "article", "newsletter" },
                .validation_focus = .{ "readability", "seo_optimization", "engagement" },
                .required_sections = .{ "title", "introduction", "conclusion" },
                .style_preferences = .{
                    .meta_description = true,
                    .featured_image = true,
                    .social_sharing = true,
                },
            },
            .process_documentation = .{
                .preferred_templates = .{ "sop", "onboarding_guide", "incident_playbook" },
                .validation_focus = .{ "step_clarity", "completeness", "actionability" },
                .required_sections = .{ "purpose", "steps", "troubleshooting" },
                .style_preferences = .{
                    .numbered_steps = true,
                    .checkboxes = true,
                    .warning_callouts = true,
                },
            },
            .rfc_specification = .{
                .preferred_templates = .{ "rfc_specification", "specification" },
                .validation_focus = .{ "rfc_2119_compliance", "normative_language", "reference_accuracy", "section_completeness" },
                .required_sections = .{ "abstract", "introduction", "requirements_language", "specification", "security_considerations", "references" },
                .style_preferences = .{
                    .rfc_2119_keywords = true,
                    .numbered_sections = true,
                    .normative_references = true,
                    .security_considerations = true,
                    .iana_considerations = true,
                    .authors_addresses = true,
                },
            },
        },
        .by_environment = .{
            .development = .{
                .debug_mode = true,
                .strict_validation = false,
                .auto_backup = true,
                .verbose_logging = true,
            },
            .production = .{
                .debug_mode = false,
                .strict_validation = true,
                .auto_backup = true,
                .performance_optimization = true,
            },
        },
    },
    .validation_rules = .{
        .content_quality = .{
            .readability = .{
                .flesch_reading_ease_min = 60,
                .avg_sentence_length_max = 20,
                .passive_voice_max_percent = 10,
                .paragraph_length_max_sentences = 6,
                .transition_words_required = true,
            },
            .accessibility = .{
                .alt_text_required = true,
                .color_contrast_check = true,
                .heading_hierarchy_strict = true,
                .descriptive_link_text = true,
                .table_headers_required = true,
            },
            .seo_optimization = .{
                .meta_description_required = true,
                .title_length_optimal = .{ .min = 30, .max = 60 },
                .keyword_density_max = 3.0,
                .internal_linking_suggested = true,
                .image_alt_text_seo = true,
            },
            .engagement = .{
                .introduction_hook_required = true,
                .clear_value_proposition = true,
                .call_to_action_suggested = true,
                .subheading_frequency_optimal = true,
            },
        },
        .technical_compliance = .{
            .markdown_standards = .{
                .commonmark_compliance = true,
                .gfm_compliance = true,
                .strict_syntax_validation = true,
                .escape_sequence_validation = true,
            },
            .link_validation = .{
                .check_external_links = true,
                .check_internal_references = true,
                .timeout_seconds = 5,
                .validate_anchors = true,
                .check_relative_paths = true,
            },
            .code_blocks = .{
                .require_language_tags = true,
                .validate_syntax = true,
                .max_line_length = 120,
                .indentation_consistency = true,
                .escape_special_chars = true,
            },
            .structure_validation = .{
                .require_h1 = true,
                .max_heading_depth = 4,
                .allow_orphan_sections = false,
                .heading_sequence_validation = true,
                .toc_accuracy_check = true,
            },
            .rfc_2119_compliance = .{
                .validate_requirement_keywords = true,
                .allowed_keywords = .{ "MUST", "MUST NOT", "REQUIRED", "SHALL", "SHALL NOT", "SHOULD", "SHOULD NOT", "RECOMMENDED", "NOT RECOMMENDED", "MAY", "OPTIONAL" },
                .require_all_caps = true,
                .require_requirements_language_section = true,
                .validate_keyword_context = true,
                .check_normative_language = true,
                .enforce_rfc_references = true,
            },
        },
        .document_type_specific = .{
            .default = .{
                .require_h1 = true,
                .max_heading_depth = 4,
                .allow_orphan_sections = false,
                .check_link_integrity = true,
                .enforce_front_matter = true,
                .spell_check = true,
                .max_line_length = 100,
                .require_empty_line_after_heading = true,
            },
            .strict = .{
                .require_h1 = true,
                .max_heading_depth = 3,
                .allow_orphan_sections = false,
                .check_link_integrity = true,
                .enforce_front_matter = true,
                .spell_check = true,
                .max_line_length = 80,
                .require_empty_line_after_heading = true,
                .require_title_case_headings = true,
                .enforce_consistent_list_markers = true,
                .require_code_language_tags = true,
            },
            .academic = .{
                .require_h1 = true,
                .max_heading_depth = 5,
                .allow_orphan_sections = false,
                .check_link_integrity = true,
                .enforce_front_matter = true,
                .spell_check = true,
                .max_line_length = 90,
                .require_empty_line_after_heading = true,
                .require_abstract = true,
                .enforce_citation_format = true,
                .bibliography_required = true,
                .in_text_citation_validation = true,
            },
            .technical = .{
                .require_h1 = true,
                .max_heading_depth = 4,
                .allow_orphan_sections = false,
                .check_link_integrity = true,
                .enforce_front_matter = true,
                .require_code_language_tags = true,
                .validate_code_syntax = true,
                .require_api_examples = true,
                .parameter_documentation_complete = true,
            },
            .process_documentation = .{
                .require_h1 = true,
                .numbered_steps_validation = true,
                .action_verb_requirements = true,
                .prerequisite_section_required = true,
                .troubleshooting_section_required = true,
                .step_completeness_check = true,
            },
            .rfc_specification = .{
                .require_h1 = true,
                .max_heading_depth = 6,
                .rfc_2119_compliance = true,
                .require_requirements_language_section = true,
                .require_security_considerations = true,
                .require_iana_considerations = true,
                .require_normative_references = true,
                .validate_rfc_keywords = true,
                .require_abstract = true,
                .numbered_sections_required = true,
            },
        },
        .validation_levels = .{
            .minimal = .{
                .basic_syntax_only = true,
                .critical_errors_only = true,
            },
            .standard = .{
                .structure_validation = true,
                .link_checking = true,
                .basic_quality_checks = true,
            },
            .comprehensive = .{
                .full_quality_analysis = true,
                .seo_optimization_checks = true,
                .accessibility_validation = true,
                .performance_analysis = true,
            },
        },
    },
